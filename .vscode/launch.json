{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pytest: test_enrollment_flow",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["-q", "tests/test_controller.py", "-k", "test_enrollment_flow", "-vv"],
      "cwd": "${workspaceFolder}",
      "env": { "PYTHONPATH": "${workspaceFolder}" },
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Pytest: PDB (test_enrollment_flow)",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["--pdb", "-q", "tests/test_controller.py", "-k", "test_enrollment_flow"],
      "cwd": "${workspaceFolder}",
      "env": { "PYTHONPATH": "${workspaceFolder}" },
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}